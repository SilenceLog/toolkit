{
  "name": "toolkit",
  "version": "0.0.1",
  "description": "javascript tool library",
  "browser": "dist/toolkit.js",
  "main": "dist/toolkit.back.js",
  "scripts": {
    "build": "webpack",
    "build:browser": "webpack --config script/browser.js",
    "build:common": "webpack --config script/common.js",
    "build:test": "webpack --config script/test.js",
    "build:util": "webpack --config script/util.js",
    "build:node": "webpack dist/build.js",
    "doc": "jsdoc -c ./conf.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SilenceLog/toolkit.git"
  },
  "keywords": [
    "toolkit",
    "tool",
    "util"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "author": "SilenceLog",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SilenceLog/toolkit/issues"
  },
  "homepage": "https://github.com/SilenceLog/toolkit#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "jest": "^22.4.2",
    "jsdoc": "^3.5.5",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "standard": "^11.0.1"
  },
  "standard": {
    "ignore": [
      "/jsdoc/",
      "/script/",
      "/static/",
      "/test/"
    ]
  }
}
